  INTEGER, INTENT(OUT) :: type           ! Selection type
                                         ! Valid values are:
                                         !    H5S_SEL_ERROR_F 
                                         !    H5S_SEL_NONE_F 
                                         !    H5S_SEL_POINTS_F 
                                         !    H5S_SEL_HYPERSLABS_F 
                                         !    H5S_SEL_ALL_F 
  INTEGER, INTENT(OUT) :: hdferr         ! Error code
END SUBROUTINE h5sget_select_type_f
    </pre>

